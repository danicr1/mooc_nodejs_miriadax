{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "no-console": 0,
        "no-param-reassign": 0,
        "no-plusplus": [
            2,
            {
                "allowForLoopAfterthoughts": true
            }
        ]
    }
}